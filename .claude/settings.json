{
  "permissions": {
    "allow": [
      "Bash(go:*)",
      "Bash(git:*)",
      "Bash(mkdir:*)",
      "Bash(./comproc:*)",
      "Bash(COMPROC_SOCKET=/tmp/comproc/test.sock go run:*)",
      "Search(*)"
